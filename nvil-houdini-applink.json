{
    "path": "$NVIL_APPLINK",
    "enable": "houdini_version >= '18.0'",
    "env": [
        {
            "nvil_applink_current_version": "MANUAL"
        },
        {
            "NVIL_APPLINK": "$HOUDINI_PACKAGE_PATH/nvil-houdini-applink"
        },
        {
            "PATH": {
                "method": "prepend",
                "value": [
                    "$NVIL_APPLINK/bin"
                ]
            }
        }
    ]
}
